{
  "name": "mithub-js",
  "version": "1.0.0",
  "description": "",
  "main": "init.js",
  "scripts": {
    "dev:client": "cd client && npm run dev:start",
    "dev:server": "cd server && npm run dev:start",
    "dev:start": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "build": "npm run build:client && npm run build:server",
    "start": "cd server && npm start",
    "prestart": "npm run build",
    "preinstall": "(cd client && npm install); (cd server && npm install);"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minimal1/mithub-js.git"
  },
  "keywords": [
    "timeline"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/minimal1/mithub-js/issues"
  },
  "homepage": "https://github.com/minimal1/mithub-js#readme",
  "dependencies": {
    "concurrently": "^5.2.0",
    "markdown-to-json": "^0.5.4",
    "mongoose": "^5.9.10",
    "remove-markdown": "^0.3.0"
  }
}
